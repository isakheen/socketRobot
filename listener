import tkinter as tk


def click_listener():
    master = tk.Tk()

    #def click(event):
    #    frame.focus_set()
    #    key2 = event.x, event.y
    #    return key2

    def char(event):
        key1 = repr(event.char)
        print("pressed", key1)
        f = open("texttest", "r")
        f.close()
        f.write(key1)

    frame = tk.Frame(master, height=500, width=500)
    #frame.bind("<Button-1>", click)
    frame.bind("<Key>", char)
    frame.pack()

    master.mainloop()
