import tkinter as tk


def click_listener():
    master = tk.Tk()

    def click(event):
        frame.focus_set()
        if event:
            print("hej")

    frame = tk.Frame(master, height=500, width=500)
    frame.bind("<Button-1>", click)
    frame.bind("<Key.enter>", char)
    frame.pack()

    master.mainloop()
